# SQRT Production Configuration
# Copy this file to .env and update values as needed

# Logging Configuration
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Gradio Server Configuration
GRADIO_SERVER_NAME=0.0.0.0
GRADIO_SERVER_PORT=7860
GRADIO_SHARE=false  # Set to 'true' to create a public shareable link

# Performance Settings
REALTIME_INTERVAL=2.0  # Seconds between real-time analyses
MAX_IMAGE_SIZE=4096  # Maximum image dimension in pixels

# Note: .env file should never be committed to version control
# It's already included in .gitignore
